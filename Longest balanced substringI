class Solution:
    def longestBalanced(self, s: str) -> int:
        n = len(s)
        ans = 0
        for i in range(n):
            freq = [0] * 26
            for j in range(i, n):
                freq[ord(s[j]) - 97] += 1
                vals = [x for x in freq if x > 0]
                if len(vals) > 0 and len(set(vals)) == 1:
                    ans = max(ans, j - i + 1)
        return ans
