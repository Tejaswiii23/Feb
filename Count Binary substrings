class Solution:
    def countBinarySubstrings(self, s: str) -> int:
        groups=[]
        c=1
        for i in range(1,len(s)):
            if s[i]==s[i-1]:
                c+=1
            else:
                groups.append(c)
                c=1
        groups.append(c)
        ans=0
        for i in range(1,len(groups)):
            ans+=min(groups[i-1],groups[i])
        return ans
        
